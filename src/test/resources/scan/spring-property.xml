<?xml version="1.0" encoding="UTF-8"?>
<beans>

    <bean class="com.lin.springframework.beans.factory.PropertyPlaceholderConfigurer">
        <property name="location" value="classpath:scan/token.properties"/>
    </bean>

    <bean id="userService" class="com.lin.springframework.scan.bean.UserService">
        <property name="token" value="${token}"/>
    </bean>

</beans>